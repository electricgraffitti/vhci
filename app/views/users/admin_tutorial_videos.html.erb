<h2 class="title title_color">Tutorials</h2>
  <table id="videos_table" class="display">
    <thead>  
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th>View Video</th>
        <th>Edit Video</th>
        <th>Delete Video</th>
      </tr>
    </thead>

    <tbody>
      <% @tutorial_videos.each do |s| %>
        <tr>
          <td><%=h s.title %></td>
          <td><%= truncate(s.description, :length => 250) %></td>
          <td><%= link_to 'View Video', tutorial_video_path(s) %></td>
          <td><%= link_to 'Update Video', edit_video_path(s) %></td>
          <td><%= link_to 'Delete Video', s, :confirm => 'Are you sure?', :method => :delete %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to 'New Video', new_tutorial_video_path %>

  <% content_for :javascripts do %>
    <script type="text/javascript" charset="utf-8">
      $(document).ready(function() {
        admin.employeeEdit();
        admin.datePickers();
        admin.dataTables();
        admin.thirdPartyUrlToggle();
        admin.multipleUploads();
      });
    </script>
  <% end %>