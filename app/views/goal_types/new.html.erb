<h1>New goal_type</h1>

<%= form_for(@goal_type) do |f| %>
  <%= render :partial => "shared/error_block", :locals => {:object => f} %>

  <p>
    <%= f.label :goal_type %><br />
    <%= f.text_field :goal_type %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', goal_types_path %>