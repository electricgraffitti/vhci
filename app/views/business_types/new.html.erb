<h1>New business_type</h1>

<%= form_for(@business_type) do |f| %>
  <%= render :partial => "shared/error_block", :locals => {:object => f} %>

  <p>
    <%= f.label :business_type %><br />
    <%= f.text_field :business_type %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', business_types_path %>