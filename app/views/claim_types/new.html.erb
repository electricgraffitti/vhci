<h1>New claim_type</h1>

<%= form_for(@claim_type) do |f| %>
  <%= render :partial => "shared/error_block", :locals => {:object => f} %>
  <p>
    <%= f.label :claim_type %><br />
    <%= f.text_field :claim_type %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', claim_types_path %>